<main>
<br>
<br>
<section id="login">
  <div class="container-fluid pt-3">
    <div class="row">
      <div class="col s4 offset-s4">
        <div class="row login-form">
          <div class="col s12">
            <div class="card hoverable">
              <div class="card-content">
                <span class="card-title center">Sign Up</span>
                <div class="divider"></div>
                <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>  
                  <div class="form-inputs row">
                     
                      
                      
                      <div class="input-field col s12 email required user_email">
                        <i class="material-icons prefix">account_box</i>
                          <%= f.text_field :name, autofocus: true, class:'validate string email required' %>  
                          <label
                          class="email required" for="user_email"><abbr
                          title="required"></abbr> Name</label>
                      </div>

                     <div class="input-field col s12 email required user_email">
                        <i class="material-icons prefix">email</i>

                          <%= f.email_field :email, autofocus: true, class:'validate string email required'%>

                          <label
                          class="email required" for="user_email"><abbr
                          title="required"></abbr> Email</label>
                      </div>
                      <div
                        class="input-field col s12 password required user_password">
                        <i class="material-icons prefix">lock</i>
                          <%= f.password_field :password, autocomplete: "off", class:'validate password required' %>

                          <label
                          class="password required" for="user_password"><abbr
                          title="required"></abbr> Password</label>
                      </div>
                    <div
                        class="input-field col s12 password required user_password">
                        <i class="material-icons prefix">lock</i>
                          <%= f.password_field :password_confirmation, autocomplete: "off", class:'validate password required' %>
                          <label
                          class="password required" for="user_password"><abbr
                          title="required"></abbr> Password</label>
                      </div>
                  </div>
                  <div class="form-actions center">
                      <i class="btn waves-effect waves-input-wrapper" style="">
                      <%= f.submit "Sign up" , class:'waves-button-input'%>
                        </i>
                    </div>
                <% end %>
                <div class="card-action">
                  <div class="devise-links">
                    <%= render "devise/shared/links" %>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</section>
</main>